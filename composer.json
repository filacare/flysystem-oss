{
    "name": "filacare/flysystem-oss",
    "description": "Aliyun OSS adapter based on flysystem, Support client side encryption, Compatible Laravel 9|10",
    "keywords": ["oss", "laravel", "encryption", "flysystem", "cloud", "storage", "filesystem", "aliyun"],
    "type": "library",
    "autoload": {
        "psr-4": {
            "Filacare\\Flysystem\\Oss\\": ""
        }
    },
    "require": {
        "php": "^8.0.2",
        "psr/log": "^1.1 || ^2.0 || ^3.0",
        "league/flysystem": "^3.10.0",
        "aliyuncs/oss-sdk-php": "^2.4"
    },
    "conflict": {
        "guzzlehttp/ringphp": "<1.1.1",
        "guzzlehttp/guzzle": "<7.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "filacare",
            "email": "git@filacare.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Filacare\\Flysystem\\Oss\\OssStorageServiceProvider"
            ]
        }
    },
    "require-dev": {
        "laravel/framework": "^10.18"
    }
}
